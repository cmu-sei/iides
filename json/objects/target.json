    "$id": "[REPLACE with full github url to this json file]",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Target",
    "description": "The system, data, person, or physical property that was targeted by the insider. An incident may have many targets.",
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "A unique string that begins with \"target--\" and is appended with a UUIDv4.",
        "pattern": "^target--[UUIDv4]"
      },
      "asset_type": {
        "type": "string",
        "description": "TODO",
        "$ref": "#/$defs/target-asset-vocab"
      },
      "category": {
        "type": "string",
        "description": "TODO",
        "$ref": "#/$defs/target-category-vocab"
      },
      "subcategory": {
        "type": "string",
        "description": "TODO",
        "$ref": "#/$defs/target-subcategory-vocab"
      },
      "format": {
        "type": "string",
        "description": "The data type of the target.",
        "$ref": "#/$defs/target-format-vocab"
      },
      "owner": {
        "type": "string",
        "description": "Who the data is about. For assets, the owner of the asset. In cases where the owner and subject of the data/asset is unclear, pick the person/group most responsible for safeguarding the data/asset.",
        "$ref": "#/$defs/target-owner-vocab"
      },
      "sensitivity": {
        "type": "string",
        "description": "TODO",
        "$ref": "#/$defs/target-sensitivity-vocab"
      },
      "description": {
        "type": "string",
        "description": "Brief description of the target."
      }
    },
    "required": [
        "id",
        "asset_type",
        "category",
        "subcategory",
        "format",
        "owner",
        "sensitivity"
    ],
    "dependentRequired": {
      "subcategory": ["category"],
      "category": ["asset_type"]
    },
